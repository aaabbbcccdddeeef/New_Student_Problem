#include<stdio.h>
 
int main(){
    int T;
    scanf("%d", &T);
    while (T--){
        int n;
        scanf("%d", &n);
        int x = 2;
        while (n > 0){
            n -= x*x;
        }
        if (n == 0){
            printf("Yes\n");
        }
        else{
            printf("No\n");
        }
    }
    return 0;
}
