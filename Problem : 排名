#include<stdio.h>
 
int main(){
    int n;
    scanf("%d",&n);
    while (n--){
        int N,M;
        scanf("%d %d",&N,&M);
        int sum = 1;
        int zqd;
        while (N--){
            scanf("%d",&zqd);
            if (zqd > M){
                sum++;
            }
        }
        printf("%d\n",sum);
    }
    return 0;
}
