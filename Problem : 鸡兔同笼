#include<stdio.h>
    int main(){
        int N;
        scanf("%d",&N);
        while (N--){
            int cnt;
            scanf("%d",&cnt);
            if (cnt % 2){
                printf("0 0\n");
                continue;
            }
            int a,b;
            a = cnt / 4;
            b = cnt - 4 * a;
            printf("%d ",a + b / 2);
            printf("%d\n",cnt / 2);
        }
        return 0;
    }
