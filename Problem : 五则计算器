#include<stdio.h>
 
int main(){
    int n;
    scanf("%d",&n);
    while (n--){
        int type;
        int a,b;
        scanf("%d",&type);
        scanf("%d %d",&a,&b);
        if (type == 1){
            printf("%d\n",a + b);
        }else if (type == 2){
            printf("%d\n",a - b);
        }else if (type == 3){
            printf("%d\n",a * b);
        }else if (type == 4){
            printf("%d\n",a / b);
        }else if (type == 5){
            printf("%d\n",a % b);
        }
    }
    return 0;
}
