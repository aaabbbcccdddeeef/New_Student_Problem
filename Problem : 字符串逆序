#include<stdio.h>
int main(){
    int n;
    scanf("%d\n",&n);
    while (n--){
        int i = 0;
        char a;
        char kirby[55] = {0};
        while ((a = getchar()) != '\n'){
            kirby[i] = a;
            i++;
        }
        i--;
        while (i != -1){
            printf("%c",kirby[i]);
            i--;
        }
        printf("\n");
    }
    return 0;
}
